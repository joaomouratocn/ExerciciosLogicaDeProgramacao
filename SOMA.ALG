algoritmo "soma"
var
   n1,n2,resultado:inteiro
inicio
      escreva("Digite um valor:")
      leia(n1)
      escreval()
      escreva("Digite um outro valor:")
      leia(n2)
      escreval()
      
      resultado <- n1 + n2
      
      escreva("O resuldato da equação é: ",resultado)
      escreval()
fimalgoritmo