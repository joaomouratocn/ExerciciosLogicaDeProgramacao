algoritmo "operadores_relacionais"
var
   n1,n2:inteiro
inicio
      escreva("Escreva o primeiro valor: ")
      leia(n1)
      escreva("Escreva o segundo valor: ")
      leia(n2)

      escreval("O primeiro e maior que o segundo: ",n1<n2)
      escreval("O segundo e marior que o primeiro: ",n2<n1)
fimalgoritmo